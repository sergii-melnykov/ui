    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-tooltip": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "vaul": "^1.1.2",
    "vite": "^6.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
